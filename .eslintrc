{
  "extends": [
    "react-app",
    "airbnb",
    "plugin:jsx-a11y/recommended",
    "prettier",
    "prettier/react"
  ],
  "plugins": [
    "jsx-a11y",
    "prettier"
  ],
  "rules": {
    "semi": 0,
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "prettier/prettier": [
      "error", {
        "semi": false
      }
    ]
  },
  "env": {
    "browser": true
  },
  "globals": {
    "shallow": true,
    "mount": true,
    "mockStore": true,
    "jsdom": true,
    "given": true,
    "when": true,
    "then": true,
    "cy": true,
    "Cypress": true
  },
  "settings": {}
}